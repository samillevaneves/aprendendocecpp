#include <cstdio>

int g(short x, short y){
    if (0 < x < y){
        return y - x;
    }else{
        return x;
    }
}

int main(){
    
    short starc, megam, koopa;
    
    scanf("%hi%hi%hi", &starc, &megam, &koopa);
    
    if(starc == 30){
        printf("PROXIMO MUNDO");
    }else{
        printf("%hi %hi %hi", g(starc, 30), g(megam, 6), g(koopa, 3));
    }
    
    return 0;
}
