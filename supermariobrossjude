#include <iostream>

int g(int x, int y){
    if (0 < x < y){
        return y - x;
    }else{
        return x;
    }
}

int main(){
    
    int starc, megam, koopa;
    
    scanf("%i%i%i", &starc, &megam, &koopa);
    
    if(starc == 30){
        printf("PROXIMO MUNDO");
    }else{
        printf("%i %i %i", g(starc, 30), g(megam, 6), g(koopa, 3));
    }
    
    return 0;
}
