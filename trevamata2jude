#include <cstdio>

int main(){

    short nmf, max; //nmf for number of mfs, max for max weight

    scanf("%hi", &nmf);

    short w[nmf]; //w for weight
    char n[nmf][50]; //n for name

    for(short i=0; i < nmf; i++){
        scanf("%s %hi", &n[i], &w[i]);
    }

    scanf("%hi", &max);

    bool ealr = true; //ealrg for everything alr

    for(short i=0; i < nmf; i++){
        if(w[i] > max){
            puts("Vamos virar almo√ßo de aranhas gigantes!");
            for(short j=0; j < nmf; j++){
                if(w[j] > max){
                    puts(n[j]);
                }
            }
            ealr = false;
            break;
        }
    }

    if(ealr){
        puts("Vamos todos encontrar a montanha!");
    }

    return 0;
}
